    "#거스름 돈의 개수를 구하는 문제\n",
    "\n",
    "m = 1320 #  받는돈\n",
    "coin_type= [500,100,50,10] # 잔돈\n",
    "count=0 # 동전 개수\n",
    "for coin in coin_type:\n",
    "  count+=m//coin # m // coin_type[0]의 첫값인 500의 목인 2를 count에 더하고 그다음 [1] 100에서의 3 50원은 생략하고 10원 2개 더해서 7개가 나온다\n",
    "  m%=coin\n",
    "\n",
    "\n",
    "print(count)\n"
   ]
